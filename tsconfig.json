{
    "$schema": "https://json.schemastore.org/tsconfig",
    
  
    "compilerOptions": {
      "allowJs": true,
      "esModuleInterop": true,
      "jsx": "react-native",
      "lib": ["DOM", "ESNext"],
      "moduleResolution": "node",
      "noEmit": true,
      "resolveJsonModule": true,
      "target": "ESNext",
      "strict": true,
      "typeRoots": ["./node_modules/@types", "./node_modules/react-native-app-auth", "./node_modules/redux"]
    },
    "exclude": [ "babel.config.js", "metro.config.js", "jest.config.js"]
  }
  